<%= back_url_hidden_field_tag %>
<%= error_messages_for 'project_enumeration' %>

<%= hidden_field_tag :custom_field_id, @project_enumeration.custom_field_id %>

<div class="box tabular">
  <p><%= f.text_field :value, :maxlength => 60, :size => 60, :required => true %></p>
<% unless @project_enumeration.new_record? %>
  <p><%= f.select :status, ProjectEnumeration::ENUMERATION_STATUSES.collect {|s| [l("version_status_#{s}"), s]} %></p>
<% end %>
  <p><%= f.select :sharing, @project_enumeration.allowed_sharings.collect {|v| [format_version_sharing(v), v]} %></p>
</div>
